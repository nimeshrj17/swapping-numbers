#include<stdio.h>
int main()
{
	int choice;
	printf("The menu:\n 1.pizza , Rs. 239 \n 2.burger , Rs.129 \n 3.pasta , Rs.179 \n 4.french fries, Rs.99\n 5.sandwich , Rs.149\n");
	printf("enter your choice\n");
	scanf("%d",&choice);
    switch (choice)
	{case 1 : printf("food item = pizza \n price -239\n");
	break;
	case 2 : printf("food item =burger\n price-129\n");
	break;
	case 3 : printf("food item =pasta\n price-179\n");
	break;
	case 4 : printf("food item =french fries\n price-99\n");
	break;
	case 5 : printf("food item =sandwich\n price-149\n");
	break;
	default:printf ("invalid choice\n");
}
return 0;}
